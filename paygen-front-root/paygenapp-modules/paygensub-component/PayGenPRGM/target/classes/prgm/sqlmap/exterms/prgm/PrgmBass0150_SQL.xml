	<select id="prgmBass0150DAO.selectBass0150List_D" parameterClass="prgmBass0150SrhVO" resultClass="egovMap">
 
				SELECT
								  DPOB_CD
								, PAYR_MANG_DEPT_CD
								, PAY_MANGE_DEPT_NM AS  PAYR_MANG_DEPT_NM
						FROM BASS0150
				WHERE 1=1
		         AND PAY_MANGE_DEPT_F_NDTN_USE_YN = 'Y'	
		         	<isNotEmpty  prepend="AND"  property="payrMangDeptCd">
					 PAYR_MANG_DEPT_CD  = #payrMangDeptCd# 
					</isNotEmpty> 
					ORDER BY 
					 DPOB_CD DESC
				   , PAYR_MANG_DEPT_CD ASC
		 
	</select>	
	
