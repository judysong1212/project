         		<mapping row="3" col="0">yeta3900Vo.pymtYrMnth</mapping>				<!-- 지급년월 -->
                <mapping row="3" col="1">yeta3900Vo.resnRegnNum</mapping>				<!-- 주민등록번호 -->
                <mapping row="3" col="2">yeta3900Vo.payCd</mapping>						<!-- 급여구분코드 -->
                <mapping row="3" col="3">yeta3900Vo.payItemCd</mapping>					<!-- 급여항목코드 -->
