	<select id="MenuServiceDAO.selectSysmMenuList_D"  parameterClass="sysm0100SchVO"  resultClass="egovMap"> 
		
	          SELECT DISTINCT A.*                                                                         
                         FROM (  
                                 SELECT 
                                    ROW_NUMBER()  OVER (PARTITION BY AA.SYS_DIV_CD ,DD.MNU_CD ORDER BY AA.USR_ID,AA.AUTH_GRP_SEIL_NUM) AS ROW_CNT 
                                   ,  AA.SYS_DIV_CD
                                   , AA.AUTH_GRP_SEIL_NUM
                                   , BB.BUSIN_DIV_CD 
                                   , BB.AUTH_GRP_NM
                                   , BB.GRDE   
                                   , BB.AUTH_GRP_USE_YN
                                   , BB.GRP_EPLN 
                                   , AA.USR_ID
                                   , AA.USR_AUTH_USE_YN 
                                   , CC.MNU_AUTH_GR_P_SCN_INQY_YN
                                   , CC.MNU_AUTH_GR_P_SCN_INPT_YN
                                   , CC.MNU_AUTH_GR_P_SCN_REVN_YN
                                   , CC.MNU_AUTH_GR_P_SCN_DEL_YN
                                   , CC.MNU_AUTH_GR_P_SCN_PRT_YN
                                   , DD.MNU_CD
                                   , DD.HHRK_MNU_CD
                                   , DD.MNU_NM
                                   , DD.MNU_ICN
                                   , DD.MNU_TMBL
                                   , DD.MNU_AUTH_FRM_CD
                                   , DD.MNU_TIP_NM
                                   , DD.MNU_SCN_FRM_PKG_NM
                                   , DD.MNU_PRNT_YN
                                   , DD.MNU_PRNT_ORD
                                   , DD.USE_YN
                                   , DD.MNU_EPLN
                                FROM SYSM0500 AA, SYSM0300 BB , SYSM0400 CC, SYSM0200 DD
                                WHERE 1=1 
                                <isNotEmpty prepend="AND" property="usrId" >
								  AA.USR_ID = #usrId#
								</isNotEmpty>  
                                AND   AA.USR_AUTH_USE_YN = 'Y'
                                AND   AA.SYS_DIV_CD = BB.SYS_DIV_CD
                                AND   AA.AUTH_GRP_SEIL_NUM = BB.AUTH_GRP_SEIL_NUM
                                AND   AA.SYS_DIV_CD = CC.SYS_DIV_CD
                                AND   AA.AUTH_GRP_SEIL_NUM = CC.AUTH_GRP_SEIL_NUM
                                AND   CC.SYS_DIV_CD = DD.SYS_DIV_CD
                                AND   CC.MNU_CD =  DD.MNU_CD
                                AND   DD.MNU_PRNT_YN = 'Y'
                                AND   DD.USE_YN = 'Y'
                                ) A                                                                                                            
                         CONNECT BY PRIOR A.MNU_CD = A.HHRK_MNU_CD                                                                        
                         START WITH A.MNU_CD = #sysDivCd#                                                                  
                         ORDER BY A.MNU_PRNT_ORD 
     
	</select>	                     
	
	
	<select id="MenuServiceDAO.selectSysm0200List_D"  parameterClass="sysm0100SchVO"  resultMap="msfSysm0200"> 
		 
	    	    SELECT DISTINCT A.*                                                                         
                         FROM (  
                                 SELECT DISTINCT
                                  ROW_NUMBER()  OVER (PARTITION BY AA.SYS_DIV_CD ,DD.MNU_CD ORDER BY AA.USR_ID,AA.AUTH_GRP_SEIL_NUM) AS ROW_CNT 
                                   , AA.SYS_DIV_CD
                                   , AA.AUTH_GRP_SEIL_NUM
                                   , BB.BUSIN_DIV_CD 
                                   , BB.AUTH_GRP_NM
                                   , BB.GRDE   
                                   , BB.AUTH_GRP_USE_YN
                                   , BB.GRP_EPLN 
                                   , AA.USR_ID
                                   , AA.USR_AUTH_USE_YN  
                                   , DD.MNU_CD
                                   , DD.HHRK_MNU_CD
                                   , DD.MNU_NM
                                   , DD.MNU_ICN
                                   , DD.MNU_TMBL
                                   , DD.MNU_AUTH_FRM_CD
                                   , DD.MNU_TIP_NM
                                   , DD.MNU_SCN_FRM_PKG_NM
                                   , DD.MNU_PRNT_YN
                                   , DD.MNU_PRNT_ORD
                                   , DD.USE_YN
                                   , DD.MNU_EPLN
                                FROM SYSM0500 AA, SYSM0300 BB , SYSM0400 CC, SYSM0200 DD
                                WHERE 1=1 
                                <isNotEmpty prepend="AND" property="usrId" >
								  AA.USR_ID = #usrId#
								</isNotEmpty>  
                                AND   AA.USR_AUTH_USE_YN = 'Y'
                                AND   AA.SYS_DIV_CD = BB.SYS_DIV_CD
                                AND   AA.AUTH_GRP_SEIL_NUM = BB.AUTH_GRP_SEIL_NUM
                                AND   AA.SYS_DIV_CD = CC.SYS_DIV_CD
                                AND   AA.AUTH_GRP_SEIL_NUM = CC.AUTH_GRP_SEIL_NUM
                                AND   CC.SYS_DIV_CD = DD.SYS_DIV_CD
                                AND   CC.MNU_CD =  DD.MNU_CD
                                AND   AA.SYS_DIV_CD = DD.SYS_DIV_CD
                                AND   DD.MNU_PRNT_YN = 'Y'
                                AND   DD.USE_YN = 'Y'
                                ) A                                                                                                            
                         CONNECT BY PRIOR A.MNU_CD = A.HHRK_MNU_CD                                                                        
                         START WITH A.MNU_CD = #sysDivCd#                                                                  
                         ORDER BY A.MNU_PRNT_ORD 
			 
	</select>	
	
	
