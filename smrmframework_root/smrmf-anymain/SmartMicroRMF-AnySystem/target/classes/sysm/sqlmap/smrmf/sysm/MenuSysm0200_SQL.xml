	<select id="msfSysm0200DAO.selectSysm0200List_D" parameterClass="msfSysm0200" resultClass="egovMap">
SELECT * FROM (
	SELECT A.*, ROWNUM RNUM FROM (
				SELECT
								SYS_DIV_CD
								, MNU_CD
								, HHRK_MNU_CD
								, MNU_NM
								, MNU_ICN
								, MNU_TMBL
								, MNU_AUTH_FRM_CD
								, MNU_TIP_NM
								, MNU_SCN_FRM_PKG_NM
								, MNU_PRNT_YN
								, MNU_PRNT_ORD
								, USE_YN
								, MNU_EPLN
								, KYBDR
								, INPT_DT
								, ISMT
								, REVN_DT
								, REVN_ADDR
								, INPT_ADDR
						FROM SYSM0200
				WHERE 1=1
				<isEqual prepend="AND" property="searchCondition" compareValue="0">
					SYS_DIV_CD = #searchKeyword#
				</isEqual>
				<isEqual prepend="AND" property="searchCondition" compareValue="1">
					MNU_CD LIKE '%' || #searchKeyword# || '%'
				</isEqual>
					ORDER BY 
						SYS_DIV_CD DESC
							, MNU_CD DESC
		
		<![CDATA[					
	) A WHERE ROWNUM <= #lastIndex#
)WHERE RNUM > #firstIndex#
]]>
	</select>	
