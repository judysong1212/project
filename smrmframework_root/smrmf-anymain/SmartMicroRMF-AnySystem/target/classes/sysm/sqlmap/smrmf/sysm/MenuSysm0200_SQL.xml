  <insert id="msfSysm0200DAO.insertSysm0200_S">
		<![CDATA[
			INSERT INTO SYSM0200 
				( SYS_DIV_CD
				  , MNU_CD
				  , HHRK_MNU_CD
				  , MNU_NM
				  , MNU_ICN
				  , MNU_TMBL
				  , MNU_AUTH_FRM_CD
				  , MNU_TIP_NM
				  , MNU_SCN_FRM_PKG_NM
				  , MNU_PRNT_YN
				  , MNU_PRNT_ORD
				  , USE_YN
				  , MNU_EPLN
				  , KYBDR
				  , INPT_DT
				  , ISMT
				  , REVN_DT
				  , REVN_ADDR
				  , INPT_ADDR )
			VALUES ( #sysDivCd#
				  , #mnuCd#
				  , #hhrkMnuCd#
				  , #mnuNm#
				  , #mnuIcn#
				  , #mnuTmbl#
				  , #mnuAuthFrmCd#
				  , #mnuTipNm#
				  , #mnuScnFrmPkgNm#
				  , #mnuPrntYn#
				  , #mnuPrntOrd#
				  , #useYn#
				  , #mnuEpln#
				  , #kybdr#
				  , TO_CHAR(SYSDATE,'YYYYMMDDHHMMSS')
				  , #ismt#
				  , #revnDt#
				  , #revnAddr#
				  , #inptAddr# )
		]]>
	</insert>
	
	<update id="msfSysm0200DAO.updateSysm0200_S">
		<![CDATA[
			UPDATE SYSM0200
			SET SYS_DIV_CD=#sysDivCd#
				, MNU_CD=#mnuCd#
				, HHRK_MNU_CD=#hhrkMnuCd#
				, MNU_NM=#mnuNm#
				, MNU_ICN=#mnuIcn#
				, MNU_TMBL=#mnuTmbl#
				, MNU_AUTH_FRM_CD=#mnuAuthFrmCd#
				, MNU_TIP_NM=#mnuTipNm#
				, MNU_SCN_FRM_PKG_NM=#mnuScnFrmPkgNm#
				, MNU_PRNT_YN=#mnuPrntYn#
				, MNU_PRNT_ORD=#mnuPrntOrd#
				, USE_YN=#useYn#
				, MNU_EPLN=#mnuEpln#
				, KYBDR=#kybdr#
				, INPT_DT=TO_CHAR(SYSDATE,'YYYYMMDDHHMMSS')
				, ISMT=#ismt#
				, REVN_DT=#revnDt#
				, REVN_ADDR=#revnAddr#
				, INPT_ADDR=#inptAddr#
						WHERE SYS_DIV_CD=#sysDivCd#
								AND MNU_CD=#mnuCd#
				]]>
	</update>
	
