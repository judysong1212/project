	<select id="sysIfBass0300DAO.selectBass0300List_D" parameterClass="sysComSearchVO" resultClass="egovMap">
SELECT * FROM (
	SELECT A.*, ROWNUM RNUM FROM (
				SELECT
								DPOB_CD
								, COMM_CD
