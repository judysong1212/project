								    , COMM_CD_NM
								</isNotEqual>	
								<isEqual   property="commUseYn" compareValue="A">
								     ,('[' || DECODE(USE_YN,'Y','Y','X') || ']' || COMM_CD_NM) AS COMM_CD_NM
